#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55f4e57e5be0 .scope module, "not_gate_4bit_tb" "not_gate_4bit_tb" 2 1;
 .timescale 0 0;
v0x55f4e57f88c0_0 .var "clk", 0 0;
v0x55f4e57f8980_0 .net "o", 3 0, L_0x55f4e57f8b40;  1 drivers
v0x55f4e57f8a40_0 .var "x", 3 0;
S_0x55f4e57e5d70 .scope module, "a0" "not_gate_4bit" 2 16, 3 1 0, S_0x55f4e57e5be0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "x";
    .port_info 1 /OUTPUT 4 "o";
L_0x55f4e57f8b40 .functor NOT 4, v0x55f4e57f8a40_0, C4<0000>, C4<0000>, C4<0000>;
v0x55f4e57aeba0_0 .net "o", 3 0, L_0x55f4e57f8b40;  alias, 1 drivers
v0x55f4e57f8780_0 .net "x", 3 0, v0x55f4e57f8a40_0;  1 drivers
    .scope S_0x55f4e57e5be0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f4e57f88c0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55f4e57e5be0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x55f4e57f88c0_0;
    %nor/r;
    %store/vec4 v0x55f4e57f88c0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55f4e57e5be0;
T_2 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55f4e57f8a40_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x55f4e57f8a40_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55f4e57f8a40_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 12 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55f4e57e5be0;
T_3 ;
    %vpi_call 2 19 "$dumpfile", "not_gate_4bit.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, v0x55f4e57f88c0_0 {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55f4e57f8a40_0 {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000010, v0x55f4e57f8980_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55f4e57e5be0;
T_4 ;
    %vpi_call 2 26 "$monitor", "At time %t, x(%b)= o(%b)", $time, v0x55f4e57f8a40_0, v0x55f4e57f8980_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "not_gate_4bit_tb.v";
