#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55fda9996f40 .scope module, "nor_gate_4bit_tb" "nor_gate_4bit_tb" 2 1;
 .timescale 0 0;
v0x55fda99aba00_0 .var "clk", 0 0;
v0x55fda99abac0_0 .net "o", 3 0, L_0x55fda99abe90;  1 drivers
v0x55fda99abb80_0 .var "x", 3 0;
v0x55fda99abc80_0 .var "y", 3 0;
S_0x55fda99970d0 .scope module, "a0" "nor_gate_4bit" 2 18, 3 1 0, S_0x55fda9996f40;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "x";
    .port_info 1 /INPUT 4 "y";
    .port_info 2 /OUTPUT 4 "o";
L_0x55fda99abd50 .functor OR 4, v0x55fda99abb80_0, v0x55fda99abc80_0, C4<0000>, C4<0000>;
L_0x55fda99abe90 .functor NOT 4, L_0x55fda99abd50, C4<0000>, C4<0000>, C4<0000>;
v0x55fda995dcf0_0 .net *"_ivl_0", 3 0, L_0x55fda99abd50;  1 drivers
v0x55fda99ab700_0 .net "o", 3 0, L_0x55fda99abe90;  alias, 1 drivers
v0x55fda99ab7e0_0 .net "x", 3 0, v0x55fda99abb80_0;  1 drivers
v0x55fda99ab8a0_0 .net "y", 3 0, v0x55fda99abc80_0;  1 drivers
    .scope S_0x55fda9996f40;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55fda99aba00_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55fda99abb80_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55fda99abc80_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x55fda9996f40;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x55fda99aba00_0;
    %nor/r;
    %store/vec4 v0x55fda99aba00_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55fda9996f40;
T_2 ;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55fda99abb80_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x55fda99abc80_0, 0, 4;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x55fda99abb80_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 6, 0, 4;
    %store/vec4 v0x55fda99abc80_0, 0, 4;
    %delay 10, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55fda9996f40;
T_3 ;
    %vpi_call 2 21 "$dumpfile", "nor_gate_4bit.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, v0x55fda99aba00_0 {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55fda99abb80_0 {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000010, v0x55fda99abc80_0 {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000011, v0x55fda99abac0_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55fda9996f40;
T_4 ;
    %vpi_call 2 29 "$monitor", "At time %t, x(%b), y(%b) = o(%b)", $time, v0x55fda99abb80_0, v0x55fda99abc80_0, v0x55fda99abac0_0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "nor_gate_4bit_tb.v";
    "nor_gate_4bit.v";
